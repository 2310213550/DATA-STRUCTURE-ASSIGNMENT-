#include <stdio.h>
#include <ctype.h>

char stack[50];
int top = -1;

int prec(char c) {
    if (c == '+' || c == '-') return 1;
    if (c == '*' || c == '/') return 2;
    return 0;
}

void push(char c) { stack[++top] = c; }
char pop() { return stack[top--]; }

int main() {
    char infix[50], postfix[50];
    int i, k = 0;
    printf("Enter infix: ");
    scanf("%s", infix);

    for (i = 0; infix[i]; i++) {
        if (isalnum(infix[i]))
            postfix[k++] = infix[i];
        else {
            while (top != -1 && prec(stack[top]) >= prec(infix[i]))
                postfix[k++] = pop();
            push(infix[i]);
        }
    }
    while (top != -1)
        postfix[k++] = pop();

    postfix[k] = '\0';
    printf("Postfix: %s\n", postfix);
    return 0;
}
